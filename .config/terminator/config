  suppress_multiple_term_dialog = True
  title_font = Monospace 15
  title_use_system_font = False
[keybindings]
  move_tab_left = <Primary><Shift>braceleft
  move_tab_right = <Primary><Shift>braceright
  next_tab = <Primary><Shift>l
  prev_tab = <Primary><Shift>h
